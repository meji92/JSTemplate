{
  "name": "jstemplate",
  "version": "1.0.0",
  "description": "JS template for new projects",
  "main": "index.js",
  "scripts": {
    "prestart": "node buildScripts/startMessage.js",
    "startAndCheck": "npm-run-all --parallel security-check lint:watch open:src",
    "start": "npm-run-all --parallel open:src",
    "open:src": "node buildScripts/srcServer.js",
    "security-check": "nsp check",
    "lint": "esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meji92/JSTemplate.git"
  },
  "author": "Javier Mateos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/meji92/JSTemplate/issues"
  },
  "homepage": "https://github.com/meji92/JSTemplate#readme",
  "dependencies": {
    "chalk": "^2.3.1",
    "express": "^4.16.2",
    "npm-run-all": "^4.1.2",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.3",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^2.0.5"
  }
}
